{
  "name": "dreamyroom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:levels && npm run build:blog && next build && npm run post-build",
    "post-build": "node scripts/post-build.js",
    "postbuild": "next-sitemap --config next-sitemap.config.js && cp public/robots.txt out/robots.txt 2>/dev/null && cp public/sitemap.xml out/sitemap.xml 2>/dev/null && echo LLM-Content: https://knitoutlevel.com/llms.txt >> out/robots.txt && echo LLM-Full-Content: https://knitoutlevel.com/llms-full.txt >> out/robots.txt",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "npx prettier --write .",
    "build:blog": "ts-node --project scripts/tsconfig.json scripts/generate-blog-cache.ts",
    "build:levels": "ts-node --project scripts/tsconfig.json scripts/generate-levels-json.ts",
    "translate": "ts-node --project scripts/tsconfig.json scripts/translate-i18n.ts",
    "translateBlog": "ts-node --project scripts/tsconfig.json scripts/translate-blogs.ts",
    "download:thumbnails": "npm run build:levels && node --expose-gc scripts/download-youtube-thumbnails.js \"Digimon Story Time Stranger\" \"knitoutlevel.com\"",
    "download:digimons": "node scripts/download-digimon-images.js",
    "clean:thumbnails-simple": "node -e \"const fs=require('fs'),path=require('path');const dir='public/images/thumbnails';try{const files=fs.readdirSync(dir).filter(f=>f.includes('-temp-'));files.forEach(f=>{try{fs.unlinkSync(path.join(dir,f));console.log('✓ 已删除:',f)}catch(e){console.log('⚠ 无法删除:',f)}});console.log(`🧹 清理完成，处理了 ${files.length} 个临时文件`)}catch(e){console.log('⚠ 清理出错:',e.message)}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
